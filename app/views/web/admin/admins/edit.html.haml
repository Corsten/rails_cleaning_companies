= simple_form_for @admin, url: admin_admin_path(@admin.id) do |f|
  = f.input :surname
  = f.input :name
  = f.input :patronymic
  = f.input :email
  = f.input :role
  = f.input :password
  = f.button :submit, :class => "btn btn-primary"
  = link_to t('.cancel'), admin_admins_path, :class => "btn btn-primary"
